<div style="width: 100%; float: left;">
    <h2 style="margin-left: 22%; margin-top: 3%;"></h2>
    <% if(user.userid == null && seller.sellerId != null) { %>
    <a href="/product/product_insert"><input type="button" class="btn peach" value="상품등록"
            style="margin-left: 73%; "></a>
    <% } %>
    <div class="row" style="margin-top: 3%; margin-left: 20%">

            <div class="col-lg-9 col-md-4 mb-4">
                <div class="col-lg-12">
                    <div class="row" style="margin-top: 5%">
                            <% for(var i = 0; i < item.length; i++){%>
                    <div class="col-lg-3 col-md-6 mb-4">
                        <div class="card h-100">
                            <a href="/product/product_detail/<%= item[i].productNum%>"><img class="card-img-top" src="../../<%= item[i].productImg %>" alt=""></a>
                            <div class="card-body">
                                <h4 class="card-title">
                                    <a href="/product/product_detail/<%= item[i].productNum%>"><%= item[i].productName %></a>
                                </h4>
                                <h5><%= item[i].price %>원</h5>
                            </div>
                        </div>
                    </div>
                    <% } %>
                </div>
            </div>
        </div>
    </div>
</div>